{
  "critics": {
    "lint": {
      "engine": "eslint",
      "configLocation": ".eslintrc.js",
      "ignoreLocation": ".eslintignore"
    }
  },
  "dependencies": {
    "unused-ignores": [
      "eslint"
    ],
    "mute": [
      "package-json-to-readme",
      "jsdoc-to-markdown",
      "inchjs"
    ]
  }
  "ignore": [
    "**/bin/**",
    "**/coverage/**",
    "**/lib/**",
    "**/node_modules/**",
    "**/readme/**"
  ],
  "test": [
    "**/test/unit/**",
    "**/test/functional/**"
  ]
}
