{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "5"
        }
      }
    ]
  ],
  "plugins": [
    "transform-es2015-spread",
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
