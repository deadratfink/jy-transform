{
    "name": "jy-transform",
    "description": "This project aims to read, write and transform _*.yaml_ files to _.js_ or _*.json_ files or vice-versa via CLI or API.",
    "version": "0.9.0",
    "homepage": "https://github.com/deadratfink/jy-transform",
    "author": {
        "name": "Jens Krefeldt",
        "email": "j.krefeldt@gmail.com",
        "url": "https://github.com/deadratfink"
    },
    "contributors": [
        {
            "name": "Jens Krefeldt",
            "email": "j.krefeldt@gmail.com",
            "url": "https://github.com/deadratfink"
        }
    ],
    "license": "SEE LICENSE IN [LICENSE.md](https://github.com/deadratfink/jy-transform/blob/master/LICENSE.md)",
    "repository": {
        "type": "git",
        "url": "https://github.com/deadratfink/jy-transform.git"
    },
    "publishConfig": {
        "registry": "http://npm.bdu.rocks/"
    },
    "bugs": "https://github.com/deadratfink/jy-transform/issues",
    "private": false,
    "config": {
        "test": {
            "mocha": {
                "unit": {
                    "reporter": "spec"
                }
            }
        }
    },
    "scripts": {
        "docs": "cat docs/BADGES.md > README.md && cat docs/TOC.md >> README.md && package-json-to-readme --no-footer ./package.json >> README.md && cat docs/USAGE.md >> README.md && echo '# API Reference\n' >> README.md && jsdoc2md lib/*.js index.js > docs/API.md >> README.md && doctoc README.md --github --title '# TOC' --maxlevel 2",
        "test": "istanbul cover _mocha --report lcovonly -- -R $npm_package_config_test_mocha_unit_reporter ./test/test*.js && codecov"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "dependencies": {
        "bluebird": "^3.3.3",
        "cli": "^0.11.1",
        "is-stream": "^1.0.1",
        "js-yaml": "^3.4.2",
        "json-stringify-safe": "^5.0.1",
        "mkdirp-then": "^1.2.0",
        "serialize-js": "^1.1.0"
    },
    "devDependencies": {
        "codecov": "^1.0.1",
        "doctoc": "^1.0.0",
        "fs-extra": "^0.26.7",
        "istanbul": "^0.4.2",
        "jsdoc-parse": "^1.2.7",
        "jsdoc-to-markdown": "^1.3.3",
        "mocha": "^2.3.3",
        "mocha-lcov-reporter": "^1.2.0",
        "object-path": "^0.9.2",
        "package-json-to-readme": "^1.5.0",
        "winston": "^2.2.0"
    },
    "preferGlobal": true,
    "bin": {
        "jyt": "./jyt"
    },
    "main": "./index.js",
    "keywords": [
        "jy",
        "jyt",
        "jy-transform",
        "transform",
        "convert",
        "js",
        "json",
        "yaml",
        "yaml2js",
        "yaml2json",
        "js2yaml",
        "json2yaml",
        "yamltojs",
        "yamltojson",
        "jstoyaml",
        "jsontoyaml",
        "promise"
    ]
}
